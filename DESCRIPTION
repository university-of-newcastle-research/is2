Package: is2
Title: Importance Sampling Squared
Version: 0.0.2.9001
Authors@R: c(
    person("Gavin", "Cooper", email = "gavin@gavincooper.net", role = c("aut", "cre", "trl"), comment = "Package creator and maintainer"),
    person("Reilly", "Innes", email = "Reilly.Innes@uon.edu.au", role = c("aut")),
